# Environment
ENV=development

# API Settings
API_VERSION=1.0.0
API_TITLE=ICTA API
API_DESCRIPTION=API for ICTA project
API_PREFIX=/api
DEBUG=True

# Server Settings
HOST=0.0.0.0
PORT=8000

# CORS Settings
CORS_ORIGINS=http://localhost:3000,http://localhost:8000
CORS_ALLOWED_METHODS=GET,POST,PUT,PATCH,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=*

# GPU Settings
USE_GPU=True
GPU_DEVICE=0
CUDA_VISIBLE_DEVICES=0
MIXED_PRECISION=True

# Model Settings
ACTIVE_MODELS=model_1

# Model 1 Specific Settings (with MODEL1_ prefix)
MODEL1_MODEL_NAME=KneeOsteoarthritisClassifier
MODEL1_MODEL_VERSION=1.0.0
MODEL1_BATCH_SIZE=32
MODEL1_LEARNING_RATE=0.001
MODEL1_NUM_EPOCHS=50
MODEL1_ARCHITECTURE=resnet50
